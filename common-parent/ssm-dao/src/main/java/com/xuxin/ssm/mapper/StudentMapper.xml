<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xuxin.ssm.mapper.StudentMapper" >

    <select id="findByName" resultType="student" parameterType="com.xuxin.ssm.Model.Student">
        select * from student_tb where student_name = #{student_name}
    </select>
</mapper>
